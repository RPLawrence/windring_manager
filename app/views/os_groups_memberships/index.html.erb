<p id="notice"><%= notice %></p>

<h1>Os Groups Memberships</h1>

<table>
  <thead>
    <tr>
      <th>Groupid</th>
      <th>Principalid</th>
      <th>Selectedroleid</th>
      <th>Contribution</th>
      <th>Listinprofile</th>
      <th>Acceptnotices</th>
      <th>Accesstoken</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_groups_memberships.each do |os_groups_membership| %>
      <tr>
        <td><%= os_groups_membership.GroupID %></td>
        <td><%= os_groups_membership.PrincipalID %></td>
        <td><%= os_groups_membership.SelectedRoleID %></td>
        <td><%= os_groups_membership.Contribution %></td>
        <td><%= os_groups_membership.ListInProfile %></td>
        <td><%= os_groups_membership.AcceptNotices %></td>
        <td><%= os_groups_membership.AccessToken %></td>
        <td><%= link_to 'Show', os_groups_membership %></td>
        <td><%= link_to 'Edit', edit_os_groups_membership_path(os_groups_membership) %></td>
        <td><%= link_to 'Destroy', os_groups_membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Os Groups Membership', new_os_groups_membership_path %>
