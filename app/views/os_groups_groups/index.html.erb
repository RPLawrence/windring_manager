<p id="notice"><%= notice %></p>

<h1>Os Groups Groups</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Name</th>
      <th>Charter</th>
      <th>Insigniaid</th>
      <th>Founderid</th>
      <th>Membershipfee</th>
      <th>Openenrollment</th>
      <th>Showinlist</th>
      <th>Allowpublish</th>
      <th>Maturepublish</th>
      <th>Ownerroleid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_groups_groups.each do |os_groups_group| %>
      <tr>
        <td><%= os_groups_group.Location %></td>
        <td><%= os_groups_group.Name %></td>
        <td><%= os_groups_group.Charter %></td>
        <td><%= os_groups_group.InsigniaID %></td>
        <td><%= os_groups_group.FounderID %></td>
        <td><%= os_groups_group.MembershipFee %></td>
        <td><%= os_groups_group.OpenEnrollment %></td>
        <td><%= os_groups_group.ShowInList %></td>
        <td><%= os_groups_group.AllowPublish %></td>
        <td><%= os_groups_group.MaturePublish %></td>
        <td><%= os_groups_group.OwnerRoleID %></td>
        <td><%= link_to 'Show', os_groups_group %></td>
        <td><%= link_to 'Edit', edit_os_groups_group_path(os_groups_group) %></td>
        <td><%= link_to 'Destroy', os_groups_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Os Groups Group', new_os_groups_group_path %>
