<p id="notice"><%= notice %></p>

<h1>Grid Users</h1>

<table>
  <thead>
    <tr>
      <th>Homeregionid</th>
      <th>Homeposition</th>
      <th>Homelookat</th>
      <th>Lastregionid</th>
      <th>Lastposition</th>
      <th>Lastlookat</th>
      <th>Online</th>
      <th>Login</th>
      <th>Logout</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grid_users.each do |grid_user| %>
      <tr>
        <td><%= grid_user.HomeRegionID %></td>
        <td><%= grid_user.HomePosition %></td>
        <td><%= grid_user.HomeLookAt %></td>
        <td><%= grid_user.LastRegionID %></td>
        <td><%= grid_user.LastPosition %></td>
        <td><%= grid_user.LastLookAt %></td>
        <td><%= grid_user.Online %></td>
        <td><%= grid_user.Login %></td>
        <td><%= grid_user.Logout %></td>
        <td><%= link_to 'Show', grid_user %></td>
        <td><%= link_to 'Edit', edit_grid_user_path(grid_user) %></td>
        <td><%= link_to 'Destroy', grid_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grid User', new_grid_user_path %>
