<p id="notice"><%= notice %></p>

<h1>Os Groups Notices</h1>

<table>
  <thead>
    <tr>
      <th>Groupid</th>
      <th>Tmstamp</th>
      <th>Fromname</th>
      <th>Subject</th>
      <th>Message</th>
      <th>Hasattachment</th>
      <th>Attachmenttype</th>
      <th>Attachmentname</th>
      <th>Attachmentitemid</th>
      <th>Attachmentownerid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_groups_notices.each do |os_groups_notice| %>
      <tr>
        <td><%= os_groups_notice.GroupID %></td>
        <td><%= os_groups_notice.TMStamp %></td>
        <td><%= os_groups_notice.FromName %></td>
        <td><%= os_groups_notice.Subject %></td>
        <td><%= os_groups_notice.Message %></td>
        <td><%= os_groups_notice.HasAttachment %></td>
        <td><%= os_groups_notice.AttachmentType %></td>
        <td><%= os_groups_notice.AttachmentName %></td>
        <td><%= os_groups_notice.AttachmentItemID %></td>
        <td><%= os_groups_notice.AttachmentOwnerID %></td>
        <td><%= link_to 'Show', os_groups_notice %></td>
        <td><%= link_to 'Edit', edit_os_groups_notice_path(os_groups_notice) %></td>
        <td><%= link_to 'Destroy', os_groups_notice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Os Groups Notice', new_os_groups_notice_path %>
