<p id="notice"><%= notice %></p>

<h1>Regionenvironments</h1>

<table>
  <thead>
    <tr>
      <th>Llsd settings</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regionenvironments.each do |regionenvironment| %>
      <tr>
        <td><%= regionenvironment.llsd_settings %></td>
        <td><%= link_to 'Show', regionenvironment %></td>
        <td><%= link_to 'Edit', edit_regionenvironment_path(regionenvironment) %></td>
        <td><%= link_to 'Destroy', regionenvironment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regionenvironment', new_regionenvironment_path %>
