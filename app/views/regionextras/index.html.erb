<p id="notice"><%= notice %></p>

<h1>Regionextras</h1>

<table>
  <thead>
    <tr>
      <th>Regionid</th>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regionextras.each do |regionextra| %>
      <tr>
        <td><%= regionextra.RegionID %></td>
        <td><%= regionextra.Name %></td>
        <td><%= regionextra.value %></td>
        <td><%= link_to 'Show', regionextra %></td>
        <td><%= link_to 'Edit', edit_regionextra_path(regionextra) %></td>
        <td><%= link_to 'Destroy', regionextra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regionextra', new_regionextra_path %>
