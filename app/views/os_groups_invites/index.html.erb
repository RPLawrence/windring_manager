<h1>Os Groups Invites</h1>

<table>
  <thead>
    <tr>
      <th>Groupid</th>
      <th>Roleid</th>
      <th>Principalid</th>
      <th>Tmstamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_groups_invites.each do |os_groups_invite| %>
      <tr>
        <td><%= os_groups_invite.GroupID %></td>
        <td><%= os_groups_invite.RoleID %></td>
        <td><%= os_groups_invite.PrincipalID %></td>
        <td><%= os_groups_invite.TMStamp %></td>
        <td><%= link_to 'Show', os_groups_invite %></td>
        <td><%= link_to 'Edit', edit_os_groups_invite_path(os_groups_invite) %></td>
        <td><%= link_to 'Destroy', os_groups_invite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

